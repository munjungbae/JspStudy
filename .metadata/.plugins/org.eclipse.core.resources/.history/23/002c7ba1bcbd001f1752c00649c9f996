package com.kh.dev.student.action;

import java.io.IOException;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

import com.kh.dev.student.control.ActionForward;
import com.kh.dev.student.model.StudentDAO;
import com.kh.dev.student.model.StudentVO;

public class DeleteProcAction implements Action {

	@Override
	public ActionForward execute(HttpServletRequest request, HttpServletResponse response) throws IOException {
		request.setCharacterEncoding("UTF-8");

		String id = (String) session.getAttribute("id");
		String sessionPass = (String) session.getAttribute("pass");
		String pass = request.getParameter("pass");

		svo.setId(id);
		svo.setPass(pass);
		boolean flag = sdao.deleteDB(svo);
		<jsp:useBean id="sdao" class="co.kh.dev.memberone.model.StudentDAO" />
		<jsp:useBean id="svo" class="co.kh.dev.memberone.model.StudentVO" />
		return new ActionForward("/mvcmem/loginProc.jsp", false);
	}

}
